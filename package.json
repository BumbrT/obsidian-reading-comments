		"@types/jest": "^29.2.6",
		"@types/marked": "^4.0.8",
		"@types/node": "^16.11.6",
		"@types/uuid": "^9.0.0",
		"@types/xmldom": "^0.1.31",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"@vicons/material": "^0.12.0",
		"@vicons/utils": "^0.1.4",
		"@vue/compiler-sfc": "^3.2.45",
		"builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "^29.4.2",
		"naive-ui": "^2.34.3",
